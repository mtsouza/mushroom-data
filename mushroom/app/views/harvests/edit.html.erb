<h1>Edit Harvest</h1>

</h1>


<%= form_for @harvest do |f| %>
	<p>
		<%= f.label :harvest %> <br/>
		<%= f.date_field :harvest %>
	</p>

	<p>
		<%= f.label :date %> <br/>
		<%= f.date_field :date %>
	</p>

	<p>
		<%= f.label :bagid %> <br/>
		<%= f.text_area :bagid %>
	</p>

	<p>
		<%= f.label :weight %> <br/>
		<%= f.text_area :weight %>
	</p>

	<p>
		<%= f.label :comment %> <br/>
		<%= f.text_area :comment %>
	</p>

	<p>
		<%= f.submit "Update Harvest" %>
	</p>

	
<% end %>